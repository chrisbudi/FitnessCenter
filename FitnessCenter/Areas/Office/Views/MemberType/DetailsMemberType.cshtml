@model DataObjects.Entities.tMemberType
@{
    ViewBag.Title = "DetailsMemberType";
}

<!-- BEGIN PAGE HEADER-->
<div class="page-head">
    <!-- BEGIN PAGE TITLE -->
    <div class="page-title">
        <h1>Details Master <small>Tipe Member</small></h1>
    </div>
</div>

@Html.Partial("_pageBreadcrumbs")
<!-- END PAGE HEADER-->
<!-- BEGIN PAGE CONTENT-->
<div class="row">
    <div class="col-md-12">
        <div class="portlet light">
            <div class="portlet-body">
                <div class="form-body help-block">
                    <div class="form-group">
                        @Html.LabelFor(m => m.MemberType, new { @class = "col-md-2 control-label" })
                        <div class="col-md-5">
                            <p class="form-control-static bold">
                                : @Html.DisplayFor(m => m.MemberType)
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.Biaya, new { @class = "col-md-2 control-label" })
                        <div class="col-md-5">
                            <p class="form-control-static bold">
                                : @Html.DisplayFor(m => m.Biaya)
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.JmlBulan, new { @class = "col-md-2 control-label" })
                        <div class="col-md-5">
                            <p class="form-control-static bold">
                                : @Html.DisplayFor(m => m.JmlBulan)
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.IsPaket, new { @class = "col-md-2 control-label" })
                        <div class="col-md-5">
                            <p class="form-control-static bold">
                                : @Html.DisplayFor(m => m.IsPaket)
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.Share, new { @class = "col-md-2 control-label" })
                        <div class="col-md-5">
                            <p class="form-control-static bold">
                                : @Html.DisplayFor(m => m.Share)
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.Periode, new { @class = "col-md-2 control-label" })
                        <div class="col-md-5">
                            <p class="form-control-static bold">
                                : @Html.DisplayFor(m => m.Periode)
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.LocationID, new { @class = "col-md-2 control-label" })
                        <div class="col-md-5">
                            <p class="form-control-static bold">
                                : @Html.DisplayFor(m => m.LocationID)
                            </p>
                        </div>
                    </div>
                </div>
                <div class="form-actions">
                    <div class="row">
                        <div class="col-md-9">
                            <a href="@Url.Action("Index", "MemberType")" class="btn btn-circle green bold"><i class="fa fa-arrow-left"></i> Kembali</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END  FORM PORTLET-->
    </div>
</div>

<!-- END PAGE CONTENT-->
@section Scripts {

    <script>

        $(function () {
            //Metronic.init(); // init metronic core components
