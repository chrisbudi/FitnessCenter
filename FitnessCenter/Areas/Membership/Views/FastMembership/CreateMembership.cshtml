                                    $.validator.unobtrusive.parse("form");
                                    $('.paymentMSTglMulai').val(moment().format('DD/MM/YYYY'));
                                }
                            }
                        });
                    }).fail(function (error) {
                        console.log(error);
                    }).always(function () {
                        // Or use always if you want to do the same thing
                        // whether the call succeeds or fails
                    });
                }
            });

